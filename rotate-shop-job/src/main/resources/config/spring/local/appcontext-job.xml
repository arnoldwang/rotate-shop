<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="messageProcessTask" class="com.dianping.rotate.shop.task.MessageProcessTask" init-method="run">
        <property name="runners">
            <list>
                <bean class="com.dianping.rotate.shop.service.impl.message.runnerDel.ShopAddMessageRunner"/>
                <bean class="com.dianping.rotate.shop.service.impl.message.runnerDel.CategoryMessageRunner"/>
                <bean class="com.dianping.rotate.shop.service.impl.message.runnerDel.RegionMessageRunner"/>
                <bean class="com.dianping.rotate.shop.service.impl.message.runnerDel.ShopMergeMessageRunner"/>
                <bean class="com.dianping.rotate.shop.service.impl.message.runnerDel.ShopMergeRecoverMessageRunner"/>
                <bean class="com.dianping.rotate.shop.service.impl.message.runnerDel.ShopStatusMessageRunner"/>
                <bean class="com.dianping.rotate.shop.service.impl.message.runnerDel.ShopUpdateMessageRunner"/>
                <bean class="com.dianping.rotate.shop.service.impl.message.runnerDel.ShopAddBySystemMessageRunner"/>
                <bean class="com.dianping.rotate.shop.service.impl.message.runnerDel.ShopStatusBySystemMessageRunner"/>
                <bean class="com.dianping.rotate.shop.service.impl.message.runnerDel.ShopUpdateBySystemMessageRunner"/>
            </list>
        </property>
    </bean>

</beans>